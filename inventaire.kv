# kivy 1.9.1
# vim:sw=4:fdm=marker:nowrap

<InventaireGui>: #{{{1 Fenetre de l'application
    canvas:
        Color:
            rgba: 1, 0, 0, 0 #.15
        Rectangle:
            size: self.size
            pos: self.pos
    TabbedPanelLotB:
        center: self.parent.center
        width: 768
        height: 480
#}}}

